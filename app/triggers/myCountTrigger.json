{
    "name": "myCountTrigger",
    "type": "DATABASE",
    "config": {
        "operation_types": [
            "INSERT",
            "UPDATE"
        ],
        "database": "sample_analytics",
        "collection": "transactions",
        "service_name": "mongodb-atlas",
        "match": {
            "fullDocument.transaction_count": {
                "$numberInt": "66"
            }
        },
        "project": {},
        "full_document": true,
        "full_document_before_change": false,
        "unordered": false,
        "skip_catchup_events": false
    },
    "disabled": false,
    "event_processors": {
        "FUNCTION": {
            "config": {
                "function_name": "countDocuments"
            }
        }
    }
}
